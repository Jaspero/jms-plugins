{
  "name": "jms-plugins",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:notes && npm run build:quick-edit && npm run build:help && npm run build:github-issues",
    "build:notes": "ng build --prod notes && cp ./README.md ./dist/@jaspero/jmsp-notes && cp LICENSE ./dist/@jaspero/jmsp-notes",
    "build:quick-edit": "ng build --prod quick-edit && cp ./README.md ./dist/@jaspero/jmsp-quick-edit && cp LICENSE ./dist/@jaspero/jmsp-quick-edit",
    "build:help": "ng build --prod help && cp ./README.md ./dist/@jaspero/jmsp-help && cp LICENSE ./dist/@jaspero/jmsp-help",
    "build:github-issues": "ng build --prod github-issues && cp ./README.md ./dist/@jaspero/jmsp-github-issues && cp LICENSE ./dist/@jaspero/jmsp-github-issues",
    "test": "ng test",
    "lint": "ng lint",
    "semantic-release": "multi-semantic-release --debug"
  },
  "dependencies": {
    "@angular/animations": "12.1.0",
    "@angular/cdk": "12.1.0",
    "@angular/common": "12.1.0",
    "@angular/compiler": "12.1.0",
    "@angular/core": "12.1.0",
    "@angular/elements": "12.1.0",
    "@angular/fire": "6.1.4",
    "@angular/forms": "12.1.0",
    "@angular/material": "11.0.3",
    "@angular/platform-browser": "12.1.0",
    "@angular/platform-browser-dynamic": "12.1.0",
    "@angular/router": "12.1.0",
    "@jaspero/fb-tinymce": "^1.4.1",
    "@jaspero/form-builder": "3.13.0",
    "@jaspero/ng-helpers": "4.3.0",
    "@ngneat/transloco": "2.20.0",
    "document-register-element": "1.14.10",
    "firebase": "8.6.8",
    "rxjs": "6.5.3",
    "tslib": "2.3.0",
    "zone.js": "0.11.4",
    "core-js": "3.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.1.0",
    "@angular/cli": "12.1.0",
    "@angular/compiler-cli": "12.1.0",
    "@qiwi/multi-semantic-release": "3.14.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.9",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/jasmine": "3.5.0",
    "@types/node": "12.11.1",
    "codelyzer": "6.0.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "ng-packagr": "12.1.0",
    "protractor": "7.0.0",
    "semantic-release": "17.3.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.0",
    "typescript": "4.3.4"
  },
  "workspaces": [
    "projects/*"
  ]
}
