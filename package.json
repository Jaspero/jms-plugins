{
  "name": "jms-plugins",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:notes && npm run build:quick-edit && npm run build:help && npm run build:github-issues",
    "build:notes": "ng build --configuration production notes && cp ./README.md ./dist/@jaspero/jmsp-notes && cp LICENSE ./dist/@jaspero/jmsp-notes",
    "build:quick-edit": "ng build --configuration production quick-edit && cp ./README.md ./dist/@jaspero/jmsp-quick-edit && cp LICENSE ./dist/@jaspero/jmsp-quick-edit",
    "build:help": "ng build --configuration production help && cp ./README.md ./dist/@jaspero/jmsp-help && cp LICENSE ./dist/@jaspero/jmsp-help",
    "build:github-issues": "ng build --configuration production github-issues && cp ./README.md ./dist/@jaspero/jmsp-github-issues && cp LICENSE ./dist/@jaspero/jmsp-github-issues",
    "test": "ng test",
    "lint": "ng lint",
    "semantic-release": "multi-semantic-release --debug"
  },
  "dependencies": {
    "@angular/animations": "13.3.4",
    "@angular/cdk": "13.3.4",
    "@angular/common": "13.3.4",
    "@angular/compiler": "13.3.4",
    "@angular/core": "13.3.4",
    "@angular/elements": "13.3.4",
    "@angular/fire": "7.3.0",
    "@angular/forms": "13.3.4",
    "@angular/material": "13.3.4",
    "@angular/platform-browser": "13.3.4",
    "@angular/platform-browser-dynamic": "13.3.4",
    "@angular/router": "13.3.4",
    "@jaspero/fb-tinymce": "2.6.9",
    "@jaspero/form-builder": "5.7.1",
    "@jaspero/ng-helpers": "5.2.0",
    "@jaspero/utils": "1.7.1",
    "@ngneat/transloco": "4.0.0",
    "core-js": "3.22.2",
    "document-register-element": "1.14.10",
    "firebase": "9.6.11",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.3",
    "@angular/cli": "13.3.3",
    "@angular/compiler-cli": "13.3.4",
    "@qiwi/multi-semantic-release": "6.1.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jasmine": "4.0.3",
    "@types/node": "17.0.26",
    "codelyzer": "6.0.2",
    "jasmine-core": "4.1.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.19",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-packagr": "13.3.1",
    "protractor": "7.0.0",
    "semantic-release": "19.0.2",
    "ts-node": "10.7.0",
    "tslint": "6.1.3",
    "typescript": "4.6.3"
  },
  "workspaces": [
    "projects/*"
  ]
}
